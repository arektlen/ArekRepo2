<extra:Window x:Class="OrdersRegistration.WPF.AddCustomer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:extra="http://schemas.extra.com/ui"
        xmlns:local="clr-namespace:OrdersRegistration.WPF"
        mc:Ignorable="d"
        Title="Dodaj zleceniodawcę" Height="300" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="btnAddCustomer" Content="Dodaj zleceniodawcę" HorizontalAlignment="Right" Margin="0,0,10,23" VerticalAlignment="Bottom" Width="145" Height="36" Click="btnAddCustomer_Click"/>
        <TextBox x:Name="tBAddCustomer" HorizontalAlignment="Right" Height="23" Margin="0,0,42,158" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="220"/>
        <Label x:Name="label" Content="Podaj nazwę zleceniodawcy" HorizontalContentAlignment="Left" Margin="0,0,42,191" Foreground="White" Height="27" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="220"/>
    </Grid>
</extra:Window>
